{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": ["templates"]
  },
  "productName": "Supawatch",
  "version": "0.1.0",
  "identifier": "com.supawatch.app",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "title": "Supawatch",
        "width": 380,
        "height": 500,
        "decorations": false,
        "transparent": true,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  }
}
